{
  "name": "slash-graphql",
  "description": "Command Line Tools to Manage Slash GraphQL",
  "version": "1.15.1",
  "author": "Dgraph Labs @dgraphlabs",
  "bin": {
    "slash-graphql": "./bin/run"
  },
  "bugs": "https://discuss.dgraph.io/c/issues/slash/41",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.0",
    "@oclif/plugin-update": "^1.3.10",
    "cli-ux": "^5.4.10",
    "get-stdin": "^8.0.0",
    "graphql": "^15.3.0",
    "loadash": "^1.0.0",
    "node-fetch": "^2.6.0",
    "open": "^7.1.0",
    "rimraf": "^3.0.2",
    "sleep-promise": "^8.0.1",
    "tslib": "^1.13.0",
    "yaml": "^1.10.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.6",
    "@types/chai": "^4.2.12",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.28",
    "@types/node-fetch": "^2.5.7",
    "aws-sdk": "^2.757.0",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">=10.1.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/dgraph-io/slash-graphql-cli",
  "keywords": [
    "oclif"
  ],
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "slash-graphql",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-update"
    ],
    "macos": {
      "identifier": "io.dgraph.cloud.cli"
    },
    "update": {
      "s3": {
        "bucket": "slashgraphql-cli"
      }
    }
  },
  "repository": "dgraph-io/slash-graphql-cli",
  "scripts": {
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rimraf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
